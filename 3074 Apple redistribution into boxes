class Solution {
    public int minimumBoxes(int[] apple, int[] capacity) {
        int total = 0;
        for(int x : apple)
            total += x;
        Arrays.sort(capacity);
        int boxes=0,cap=0;
        for(int i=capacity.length-1;i>=0;i--) {
            cap += capacity[i];
            boxes++;
            if(cap >= total)
                break;
        }
        return boxes;
    }
}
